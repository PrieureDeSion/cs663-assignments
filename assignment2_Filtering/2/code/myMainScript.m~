%% MyMainScript

img = load('../data/barbara.mat');
img = img.imageOrig;
noise = randn(size(img, 1)) * 0.05 * max(max(img));
imgn = max(0, img+noise);

log = zeros([size([3:2:18]), 2]);
i 
for sx = [3:2:18]
    for sr = [3:2:18]
        sx, sr
        tic;
        im_new = bilinear_filter(imgn, [sx, sx, sr]);
        toc;
        error = rmsd(img, im_new)
    end
end